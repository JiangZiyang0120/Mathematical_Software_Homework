~:$gunplotgnuplot> set term postscript eps color solid linewidth 2 "Helvetica" 20gnuplot> set xlabel "X"gnuplot> set ylabel "Y"gnuplot> set xrange [-2:2]gnuplot> set yrange [-3:3]gnuplot> set output "report_eps.eps"gnuplot> set title "Therr cases of unary quadratic equation's root"gnuplot> set gridgnuplot> set style line 1 lw 2 lc rgb "#F62217"gnuplot> set style line 2 lw 2 lc rgb "#D4A017"gnuplot> set style line 3 lw 2 lc rgb "#2B60DE"gnuplot> f(x) = 4*x**2 + 4*x + 1gnuplot> g(x) = 4*x**2 + 4*x - 1gnuplot> h(x) = 4*x**2 + 4*x + 2gnuplot> plot g(x) ls 1 t "Two roots case", f(x) ls 2 t "One root case", h(x) ls 3 t "No root case"gnuplot> set outputgnuplot> exit